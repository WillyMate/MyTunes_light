      </AnchorPane>
      <HBox prefHeight="490.0" prefWidth="862.0">
         <children>
            <VBox prefHeight="370.0" prefWidth="205.0">
               <children>
                  <Label text="Playlist" />
                  <TableView fx:id="tblViewPlaylist" prefHeight="358.0" prefWidth="206.0">
                     <columns>
                        <TableColumn prefWidth="84.0" text="Name" />
                        <TableColumn prefWidth="41.0" text="Songs" />
                        <TableColumn prefWidth="100.0" text="Duration" />
                     </columns>
                  </TableView>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="PL_NEWBTN" mnemonicParsing="false" text="New...">
                           <HBox.margin>
                              <Insets top="5.0" />
                           </HBox.margin></Button>
                        <Button fx:id="PL_EDITBTN" mnemonicParsing="false" text="Edit...">
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" top="5.0" />
                           </HBox.margin></Button>
                        <Button fx:id="PL_DELBTN" mnemonicParsing="false" text="Delete">
                           <HBox.margin>
                              <Insets top="5.0" />
                           </HBox.margin></Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox prefHeight="370.0" prefWidth="174.0">
               <children>
                  <Label text="Songs on Playlist" />
                  <ListView fx:id="lstViewPlaylist" prefHeight="359.0" prefWidth="174.0" />
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="MoveUpBTN" mnemonicParsing="false" text="^">
                           <HBox.margin>
                              <Insets top="5.0" />
                           </HBox.margin></Button>
                        <Button fx:id="MoveDownBTN" mnemonicParsing="false" text="v">
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" top="5.0" />
                           </HBox.margin></Button>
                        <Button fx:id="PL_SO_DELBTN" mnemonicParsing="false" text="Delete">
                           <HBox.margin>
                              <Insets top="5.0" />
                           </HBox.margin></Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <AnchorPane prefHeight="182.0" prefWidth="66.0">
               <children>
                  <Button fx:id="AddBTN" layoutX="7.0" layoutY="14.0" mnemonicParsing="false" text="&lt;---" />
               </children>
            </AnchorPane>
            <VBox prefHeight="370.0" prefWidth="388.0">
               <children>
                  <Label text="Songs" />
                  <TableView fx:id="tblViewSongs" prefHeight="358.0" prefWidth="388.0">
                     <columns>
                        <TableColumn fx:id="songTitleCol" prefWidth="130.0" text="Title" />
                        <TableColumn fx:id="songArtistCol" prefWidth="108.0" text="Artist" />
                        <TableColumn fx:id="songGenreCol" prefWidth="51.0" text="Genre" />
                        <TableColumn fx:id="songDurationCol" prefWidth="98.0" text="Duration" />
                     </columns>
                  </TableView>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="SN_NEWBTN" mnemonicParsing="false" text="New...">
