<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="450" prefWidth="900.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.MyTunes_light.GUI.Controller.MyTunesController">
   <children>
      <AnchorPane prefHeight="102.0" prefWidth="640.0">
         <children>
            <Button fx:id="PauseBTN" layoutX="97.0" layoutY="25.0" mnemonicParsing="false" text="II">
               <HBox.margin>
                  <Insets />
               </HBox.margin></Button>
            <Button fx:id="RewindBTN" layoutX="40.0" layoutY="25.0" mnemonicParsing="false" text="Prev" />
            <Button fx:id="SkipBTN" layoutX="144.0" layoutY="25.0" mnemonicParsing="false" text="Next" />
            <Slider fx:id="slidrVol" layoutX="57.0" layoutY="57.0" />
            <Button fx:id="MuteBTN" layoutX="26.0" layoutY="51.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="28.0" text="Lyd" />
            <Label layoutX="351.0" layoutY="43.0" text="Now playing" />
            <TextField layoutX="691.0" layoutY="24.0" prefHeight="26.0" prefWidth="84.0" />
            <Button fx:id="SearchBTN" layoutX="775.0" layoutY="24.0" mnemonicParsing="false" text="Search" />
            <Label layoutX="663.0" layoutY="29.0" text="Filter" />
            <Label fx:id="isNowPlaying" layoutX="424.0" layoutY="43.0" />
