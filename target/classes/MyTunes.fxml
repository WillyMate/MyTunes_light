                        <Button fx:id="SN_DELBTN" mnemonicParsing="false" text="Delete">
                           <HBox.margin>
                              <Insets top="5.0" />
                           </HBox.margin></Button>
                        <Button fx:id="CloseBTN" alignment="BOTTOM_LEFT" mnemonicParsing="false" text="Close">
                           <HBox.margin>
                              <Insets left="180.0" top="5.0" />
                           </HBox.margin></Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <HBox.margin>
      <Insets />
   </HBox.margin>
</VBox>
